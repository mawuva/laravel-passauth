{
    "name": "mawuekom/laravel-passauth",
    "description": "A Bunch of authentication features for laravel",
    "keywords": [
        "mawuekom", "passauth",
        "laravel-passauth"
    ],
    "homepage": "https://github.com/mawuva/laravel-passauth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ephra√Øm Seddor",
            "email": "seddorephraim7@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mawuekom\\Passauth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mawuekom\\Passauth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mawuekom\\Passauth\\PassauthServiceProvider"
            ],
            "aliases": {
                "Passauth": "Mawuekom\\Passauth\\PassauthFacade"
            }
        }
    }
}
